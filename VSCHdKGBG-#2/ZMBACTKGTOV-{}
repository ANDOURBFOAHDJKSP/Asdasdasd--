 local SilentAimBoss = false

local Toggle = Tabs.Misc:AddToggle("SilentAimBosses", {Title = "Silent Aim Bosses", Default = false })

    Toggle:OnChanged(function(bool10)
    SilentAimBoss = bool10
    
local Player = game.Players.LocalPlayer
local Aim = function()
local Distance = math.huge 
local target = nil
 for value, index in next, game.Workspace.BossFolder:GetDescendants() do 
   if index:FindFirstChild(PrimaryPart) and Player.Character:FindFirstChild(PrimaryPart) and not index:FindFirstChild('Guns') and index.Parent.Name ~= "deadenemies" then 
     local Nearest = (index:FindFirstChild(PrimaryPart).Position - Player.Character.Head.Position).magnitude
         if Nearest < Distance then 
            Distance = Nearest
            target = index
         end
     end
   end
    return target
end

local MousePos = Player:GetMouse()
